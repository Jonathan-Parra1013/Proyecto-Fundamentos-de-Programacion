import pandas as pd 
# Leer la hoja 2 que contiene los jugadores
df = pd.read_excel('data/TABLAPREMIERE.xlsx', sheet_name='Hoja2')

Nombre_Equipos = df['Team'].unique()
print("Los Equipos que jugaron en la PREMIER LEAGUE (2024-2025) son: ")
for Equipo in sorted(Nombre_Equipos):
    print({Equipo})




