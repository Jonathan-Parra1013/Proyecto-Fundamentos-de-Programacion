import pandas as pd 
# Leer la hoja 2 que contiene los jugadores
df = pd.read_excel('data/TABLAPREMIER.xlsx', sheet_name='Hoja2')

Nombre_Equipos = df['Equipo'].unique()
print("Los Equipos que jugaron en la PREMIER LEAGUE (2024-2025) ")
print("En orden alfabetico son: ")
for Orden_Equipo in sorted(Nombre_Equipos):
    print("-",Orden_Equipo)

